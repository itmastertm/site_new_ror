        <div class="mbr-section-head">
            <h4 class="mbr-section-title mbr-fonts-style align-center mb-0 display-2">
                <strong>
                  <%= "#{@product.name_ru}" if params[:locale] == 'ru' %>
                  <%= "#{@product.name_en}" if params[:locale] == 'en' %>
                  <%= "#{@product.name_tm}" if params[:locale] == 'tm' %>    
