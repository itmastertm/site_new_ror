                                <%= "#{content.ru_name}" if params[:locale] == 'ru' %>
                                <%= "#{content.en_name}" if params[:locale] == 'en' %>
                                <%= "#{content.tm_name}" if params[:locale] == 'tm' %>
                            </strong>
                        </h5>
                        
                        
                    </div>
                    
                </div>
            </div>
            <% end %>
   
          
        </div>
    </div>
