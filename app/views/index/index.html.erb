<section class="slider1 cid-sqUhtftfxs" id="slider1-v">
    
    <div class="carousel slide" id="sqUvboAHsk" data-interval="5000">
        <ol class="carousel-indicators">
            <li data-slide-to="0" class="active" data-target="#sqUvboAHsk"></li>
            <li data-slide-to="1" data-target="#sqUvboAHsk"></li>
            <li data-slide-to="2" data-target="#sqUvboAHsk"></li>
            <li data-slide-to="3" data-target="#sqUvboAHsk"></li>
            <li data-slide-to="4" data-target="#sqUvboAHsk"></li>
        </ol>
        <div class="carousel-inner">
            <div class="carousel-item slider-image item active">
                <div class="item-wrapper">
                    <img class="d-block w-100" src="/assets/images/mbr-1900x1267.jpg" data-slide-to="0">
                    
                    <div class="carousel-caption">
                        <h5 class="mbr-section-subtitle mbr-fonts-style display-5"><strong><%= t('index.carousel')%></strong></h5>
                        
                    </div>
                </div>
            </div>
            <% @carousel.each do |slide| %>
            <div class="carousel-item slider-image item">
                <div class="item-wrapper">
                    <%= image_tag slide.image %>
                    
                    <div class="carousel-caption">
                        <h5 class="mbr-section-subtitle mbr-fonts-style display-5">
                        <strong>
                        <%= "#{slide.ru}" if params[:locale] != true and params[:locale] != 'en' and params[:locale] != 'tm' and params[:locale] != 'ru' %>
                        <%= "#{slide.ru}" if params[:locale] == 'ru' %>
                        <%= "#{slide.en}" if params[:locale] == 'en' %>
                        <%= "#{slide.tm}" if params[:locale] == 'tm' %>
                            
                        </strong></h5>
                        
                    </div>
                </div>
            </div>
            <% end %>
            
            
        </div>
        <a class="carousel-control carousel-control-prev" role="button" data-slide="prev" href="#sqUvboAHsk">
            <span class="mobi-mbri mobi-mbri-arrow-prev" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control carousel-control-next" role="button" data-slide="next" href="#sqUvboAHsk">
            <span class="mobi-mbri mobi-mbri-arrow-next" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</section>

<section class="content14 cid-sqTGy6amTw mbr-parallax-background" id="content14-r">
    <br><br><br><br><br><br>
    <div class="mbr-overlay" style="opacity: 0.8; background-color: rgb(255, 255, 255);"></div>
    
    <div class="container-fluid">
        <div class="row justify-content-center">
            <div class="col-md-12 col-lg-12">
                <% @abouts.each do |about| %>
                    <h1 class="mbr-section-title mbr-fonts-style mb-4 display-5"><strong>
                        
                        <%= simple_format about.name_ru, :style => "font-size: 2.75rem; font-family: 'Jost', sans-serif; font-weight: bold" if params[:locale] != true and params[:locale] != 'en' and params[:locale] != 'tm' and params[:locale] != 'ru' %>
                        <%= simple_format about.name_ru, :style => "font-size: 2.75; font-family: 'Jost', sans-serif; font-weight: bold" if params[:locale] == 'ru' %>
                        <%= simple_format about.name_en, :style => "font-size: 2.75; font-family: 'Jost', sans-serif; font-weight: bold" if params[:locale] == 'en' %>
                        <%= simple_format about.name_tm, :style => "font-size: 2.75; font-family: 'Jost', sans-serif; font-weight: bold" if params[:locale] == 'tm' %>

                    </strong></h1>
                <ul class="list mbr-fonts-style display-7">
                    <div>
                        
                    
                        <%= simple_format about.ru_body, :style => "font-size: 22px; font-family: 'Jost', sans-serif; line-height: 1.5;" if params[:locale] != true and params[:locale] != 'en' and params[:locale] != 'tm' and params[:locale] != 'ru' %>
                        <%= simple_format about.ru_body, :style => "font-size: 22px; font-family: 'Jost', sans-serif; line-height: 1.5;" if params[:locale] == 'ru' %>
                        <%= simple_format about.en_body, :style => "font-size: 22px; font-family: 'Jost', sans-serif; line-height: 1.5;" if params[:locale] == 'en' %>
                        <%= simple_format about.tm_body, :style => "font-size: 22px; font-family: 'Jost', sans-serif; line-height: 1.5;" if params[:locale] == 'tm' %>
                    <% end %>
                    
                </div>
        </div>
    </div>
</section>
