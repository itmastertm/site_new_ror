  <div class="container">
  <div class="col-lg-8 mx-auto mbr-form" data-form-type="formoid"> 
    <div class="dragArea row">
      <strong>
        <center>
          <h1>
    <%= t('form.formname') %>
          </h1>
    
            <%= simple_form_for @contact, :html => {:class => 'col-md col-sm-12 form-group' } do |f| %>
              <%= f.input_field :name, :required => true, :class => "form-control", placeholder: t('form.name'), :style => "font-size: 2.75; font-family: 'Jost', sans-serif; font-weight: bold" %>
              <br>
              <%= f.input_field :email, :required => true, :class => "form-control", placeholder: "E-Mail", :style => "font-size: 2.75; font-family: 'Jost', sans-serif; font-weight: bold" %>
              <br>
              <%= f.input_field :message, :as => :text, :required => true, :class => "form-control", style: 'height: 200px', placeholder: t('form.message'), :style => "font-size: 2.75; font-family: 'Jost', sans-serif; font-weight: bold" %>
             
      
            <div>
              </br>
              <%= f.button :submit, t('index.send'), :class => 'btn btn-success', style: 'width: 300px; border-radius: 30px' %>
            </div>
            <% end %>

        
        </center>
      </strong>  
    </div>  
  </div>    
</div>
